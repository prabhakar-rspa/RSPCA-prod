<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Visit_Approved_Flag__c</fullName>
    <description>Flag field to display a tick or a cross if an additional visit has been approved or not</description>
    <externalId>false</externalId>
    <formula>IF( Additional_Visit_Approved__c = &#39;Yes&#39;, IMAGE(&quot;/resource/ValidationCheckSmall&quot;, &#39;Yes&#39;, 24,24), IMAGE(&quot;/resource/ValidationCrossSmall&quot;, &#39;No&#39;, 24,24))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Additional Visit Approved</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
