<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>All_Documentation_Submitted__c</fullName>
    <description>Ticks when the following documentation as been populated:
Assessment Checklist
Assessment Report
Evidence</description>
    <externalId>false</externalId>
    <formula>NOT(ISBLANK(Evidence__c)) &amp;&amp; 
				NOT(ISBLANK(Assessment_Checklist__c)) &amp;&amp; 
								NOT(ISBLANK( Assessment_Report__c))</formula>
    <label>All Documentation Submitted</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
