<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Visit_Approved__c</fullName>
    <description>If both Office and Certifier has approved, this will result in &#39;Yes&#39;</description>
    <externalId>false</externalId>
    <formula>IF(
 ISPICKVAL(Office_Approved__c, &quot;Yes&quot;) &amp;&amp;
				 ISPICKVAL(Certifier_Approved__c, &quot;Yes&quot;), &#39;Yes&#39;, &#39;No&#39;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Additional Visit Approved</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
