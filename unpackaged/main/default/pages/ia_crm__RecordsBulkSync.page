<apex:page controller="ia_crm.RecordsBulkSyncController" showheader="true" sidebar="false" doctype="html-5.0" title="Sync records in bulk">
    <apex:stylesheet value="{!URLFOR($Resource.ia_crm__Intacct, '/assets/styles/salesforce-lightning-design-system-vf.css')}" />
    <apex:includescript value="{!URLFOR($Resource.ia_crm__Intacct, '/js/jQuery.min.js')}" />
    <apex:includescript value="{!URLFOR($Resource.ia_crm__Intacct, '/js/IntacctConfiguration.js')}" />
    <apex:includeLightning />
    <apex:slds />
    <div class="slds" style="padding-left: 20px; padding-right: 20px;">
        <apex:actionstatus id="fetchStatus">
            <apex:facet name="start">
                <div class="slds-spinner_container">
                    <div role="status" class="slds-spinner slds-spinner--medium slds-spinner--brand">
                        <span class="slds-assistive-text">{!$Label.Loading}</span>
                        <div class="slds-spinner__dot-a"></div>
                        <div class="slds-spinner__dot-b"></div>
                    </div>
                </div>
            </apex:facet>
        </apex:actionstatus>
        <div class="slds-card">
            <apex:form id="syncForm">
                <!--  <apex:actionFunction name="refreshFuncTab" action="{!controllerRefresh}" rerender="syncMassPanel, jEntryCreationProcessPanel, idAccountFieldFilters, idContractFieldFilters, syncContractButton"/> -->
                <apex:outputpanel rendered="{!len(pageError) < 1}">
                    <div class="slds-tabs--scoped">
                        <ul class="slds-tabs--scoped__nav" role="tablist">
                            <li id="tab1" class="slds-tabs--scoped__item slds-text-heading--label slds-active" title="{!$Label.Object_Mass_Sync}" role="presentation">
                                <a href="#" Id="firstTab" class="slds-tabs--scoped__link slds-active">{!$Label.Object_Mass_Sync}</a>
                            </li>
                            <li id="tab2" class="slds-tabs--scoped__item slds-text-heading--label" title="{!$Label.Order_Entry_Transaction_Sync}" role="presentation">
                                <a href="#" id="secondTab" class="slds-tabs--scoped__link slds-active">{!$Label.Order_Entry_Transaction_Sync}</a>
                            </li>
                            <apex:outputpanel rendered="{!showContractTab}">
                                <li id="tab3" class="slds-tabs--scoped__item slds-text-heading--label" title="{!$Label.Contract_Transaction_Sync}" role="presentation">
                                    <a href="#" id="thirdTab" class="slds-tabs--scoped__link slds-active">{!$Label.Contract_Transaction_Sync}</a>
                                </li>
                            </apex:outputpanel>
                            <apex:outputpanel rendered="{!isJournalEntryEnabled}">
                                <li id="tab4" class="slds-tabs--scoped__item slds-text-heading--label" title="{!$Label.Journal_Entry_Mass_Creation}" role="presentation">
                                    <a href="#" id="forthTab" class="slds-tabs--scoped__link slds-active">{!$Label.Journal_Entry_Mass_Creation}</a>
                                </li>
                            </apex:outputpanel>
                            <apex:outputpanel rendered="{!showContractTab}">
                                <li id="tab5" class="slds-tabs--scoped__item slds-text-heading--label" title="{!$Label.SFDC_Contract_Mass_Create}" role="presentation">
                                    <a href="#" id="fifthTab" class="slds-tabs--scoped__link slds-active">{!$Label.SFDC_Contract_Mass_Create}</a>
                                </li>
                            </apex:outputpanel>
                            <apex:outputpanel rendered="{!showSalesDocumentTab}">
                                <li id="tab6" class="slds-tabs--scoped__item slds-text-heading--label" title="{!$Label.Sales_Document_Mass_Creation}" role="presentation">
                                    <a href="#" id="sixTab" class="slds-tabs--scoped__link slds-active">{!$Label.Sales_Document_Mass_Creation}</a>
                                </li>
                            </apex:outputpanel>
                        </ul>
                        <div id="tab-scoped-1" class="slds-tabs_scoped__content slds-show" role="tabpanel" aria-labelledby="tab-scoped-1__item">
                            <div style="padding-left:30px;padding-right:30px;">
                                <apex:outputpanel id="pageMessages" style="width:100%">
                                    <apex:outputtext value="" rendered="{!hasPageErrors}">
                                        <div class="slds-notify slds-notify--alert slds-theme--error slds-theme--alert-texture" role="alert" id="TypeMessage">
                                            <apex:messages />
                                        </div>
                                    </apex:outputtext>
                                </apex:outputpanel>
                            </div>

                            <apex:outputpanel id="syncMassPanel">
                                <div style="padding-left:30px;padding-right:30px;">
                                    <h2 class="slds-has-divider--top-space slds-text-heading--small">{!$Label.ia_crm__Start_a_synchronization_process}</h2>
                                    <span class="slds-truncate slds-col_bump-left" style="float: right;">
                            			<div class="slds-grid">
                                			<div class="slds-col" style="width: 40px;">
			                                    <apex:image value="{!URLFOR($Resource.ia_crm__Intacct,'images/sfdc_logo.png')}" />
			                                </div>
			                                <div class="slds-col" style="width: 50px;">
			                                   <svg class="slds-button__icon" aria-hidden="true">
			                                       <span class="slds-icon_container null slds-icon__svg--default" data-reactid="2705"><svg xmlns="http://www.w3.org/2000/svg" class="slds-icon slds-icon-text-default" aria-hidden="true" viewBox="0 0 36 30" data-reactid="2706"><path d="M1.6 13.4h15.3c.4 0 .6.5.3.8l-4.4 4.4c-.3.3-.3.7 0 1l1 1c.3.3.7.3 1 0l8-8.1c.3-.3.3-.7 0-1l-8-8.1c-.3-.3-.7-.3-1 0l-1 1c-.2.3-.2.7 0 1l4.5 4.4c.2.3.1.8-.4.8H1.6c-.4 0-.7.3-.7.7v1.3c0 .4.3.8.7.8z" /></svg></span>
			                                   </svg>
			                                </div>
			                                <div class="slds-col" style="width: 100px;">
                                    			<apex:image value="{!URLFOR($Resource.ia_crm__Intacct,'images/sage-Intacct-logo.png')}" />
                                			</div>
                        				</div>
                                	</span>
                                    <!-- Select object -->
                                    <div class="slds-grid slds-grid_vertical-align-start">
                                        <div class="">
                                            <label class="slds-form-element__label" for="select-11">
                                                <p class="slds-text-body--regular slds-m-around--medium" style="margin-left: 0px;">{!$Label.Select_the_object}</p>
                                            </label>
                                            <apex:outputpanel id="idObjectselect">
                                                <div class="slds-form-element {!IF(isSelectobjectError==true,'slds-has-error' , '' )}">
                                                    <div class="slds-form-element__control">
                                                        <div class="slds-select_container slds-size--1-of-4" style="width: 200px;">
                                                            <apex:selectlist value="{!selectedObjectValue}" size="1" styleclass="slds-select">
                                                                <apex:actionsupport action="{!changesObjectValue}" event="onchange" status="fetchStatus" rerender="idFieldFilters, opportunityOETransactionOptions, idDefaultFilters, getRecordsPanel, getOpportunityRecordsPanel, pageMessages" />
                                                                <apex:selectoptions value="{!objectOptions}" />
                                                            </apex:selectlist>
                                                        </div>
                                                    </div>
                                                    <div class="slds-form-element__help" id="error-02"> {!IF(isSelectobjectError==true,selectobjectError , '' )}</div>
                                                </div>
                                            </apex:outputpanel>
                                        </div>
                                        <apex:outputpanel id="idDefaultFilters">
                                            <div class="">
                                                <fieldset class="slds-form-element">
                                                    <legend class="form-element__legend slds-form-element__label">{!$Label.ia_crm__Default_Filter} </legend>
                                                    <apex:outputpanel rendered="{!showHasSyncWithIntacct}">
                                                        <div class="slds-form-element slds-m-horizontal--medium">
                                                            <div class="slds-form-element__control">
                                                                <span class="slds-checkbox">
                                                                    <label class="slds-checkbox">
                                                                        <apex:inputcheckbox value="{!syncWithIntacct}">
                                                                            <apex:actionsupport event="onchange" status="fetchStatus" action="{!enableOrDisableLogicCondition}" rerender="idFieldFilters" />
                                                                        </apex:inputcheckbox>
                                                                        <span class="slds-checkbox--faux"></span>
                                                                        <span class="slds-form-element__label">{!$Label.Has_sync_with_Intacct_checked}</span>
                                                                    </label>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </apex:outputpanel>
                                                    <apex:outputpanel rendered="{!showIsSyncedSuccessful}">
                                                        <div class="slds-form-element slds-m-horizontal--medium">
                                                            <div class="slds-form-element__control">
                                                                <span class="slds-checkbox">
                                                                    <label class="slds-checkbox">
                                                                        <apex:inputcheckbox value="{!isSuccess}">
                                                                            <apex:actionsupport event="onchange" status="fetchStatus" action="{!enableOrDisableLogicCondition}" rerender="idFieldFilters" />
                                                                        </apex:inputcheckbox>
                                                                        <span class="slds-checkbox--faux"></span>
                                                                        <span class="slds-form-element__label">{!$Label.Is_synced_successful}</span>
                                                                    </label>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </apex:outputpanel>
                                                    <div class="slds-form-element slds-m-horizontal--medium">
                                                        <div class="slds-form-element__control">
                                                            <span class="slds-checkbox">
                                                                <label class="slds-checkbox">
                                                                    <apex:inputcheckbox value="{!hasIntacctSyncErrors}">
                                                                        <apex:actionsupport event="onchange" status="fetchStatus" action="{!enableOrDisableLogicCondition}" rerender="idFieldFilters" />
                                                                    </apex:inputcheckbox>
                                                                    <span class="slds-checkbox--faux"></span>
                                                                    <span class="slds-form-element__label">{!$Label.Has_sync_errors}</span>
                                                                </label>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </apex:outputpanel>
                                    </div>
                                    <div>
                                        <apex:outputpanel id="idFieldFilters">
                                            <apex:outputpanel rendered="{!hasDefaultFilter}">
                                                <div class="slds-form-element">
                                                    <div class="slds-grid slds-wrap slds-grid--pull-padded">
                                                        <div class="slds-col--padded slds-size--1-of-1 slds-medium-size--6-of-12 slds-large-size--6-of-12" style="margin: auto;">
                                                            <div class="slds-form-element__row" style="padding-left: 67px;">
                                                                <div align="center" style="width:78px;float: left;">
                                                                    <div class="slds-form-element__control">
                                                                        <div class="slds-select_container">
                                                                            <apex:selectlist value="{!selectedFilterBetweenFilters}" size="1" styleclass="slds-select">
                                                                                <apex:selectoptions value="{!andOrOptions}" />
                                                                            </apex:selectlist>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div>
                                                                    <label style="font-size: 14px; padding-left: 10px;" class="slds-form-element__label" for="select-12">{!$Label.Condition_between_Default_filter_and_Custom_filter}</label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
                                            <fieldset class="slds-form--compound ">
                                                <apex:outputpanel rendered="{!mapfieldsWrapSize > 0}">
                                                    <legend class="form-element__legend slds-form-element__label">{!$Label.ia_crm__Custom_filter}</legend>
                                                </apex:outputpanel>
                                                <apex:repeat value="{!mapfieldsWrap}" var="index">
                                                    <div class="slds-form-element" style="padding-left: 8px;">
                                                        <div class="slds-grid slds-wrap slds-grid--pull-padded">
                                                            <div>
                                                                <label class="slds-form-element__label" for="select-12"></label>
                                                                <div class="slds-form-element__control" style="margin-top: 15px;"> {!index}. </div>
                                                            </div>
                                                            <div class="slds-col--padded slds-size--1-of-2 slds-medium-size--3-of-12 slds-large-size--3-of-12">
                                                                <div class="slds-form-element__row ">
                                                                    <div class="slds-form-element ">
                                                                        <label class="slds-form-element__label" for="select-11">{!$Label.Field}</label>
                                                                        <div class="slds-form-element__control">
                                                                            <div class="slds-select_container">
                                                                                <apex:selectlist value="{!mapfieldsWrap[index].fromFieldAPIName}" size="1" styleclass="slds-select" disabled="{!IF(mapfieldsWrap[index].isDisabled || mapfieldsWrap[index].isDisabledField, 'true', 'false')}">
                                                                                    <apex:actionsupport event="onchange" action="{!changesfieldValue}" rerender="idFieldFilters, pageMessages" status="fetchStatus" immediate="false">
                                                                                        <apex:param name="selectedLine" value="{!index}" assignto="{!selectedLine}" />
                                                                                    </apex:actionsupport>
                                                                                    <apex:selectoptions value="{!fieldsOptions}" />
                                                                                </apex:selectlist>
                                                                            </div>
                                                                            <apex:outputpanel >
                                                                                <div class="slds-has-error">
                                                                                    <div id="error-message" class="slds-form-element__help">
                                                                                        <apex:outputpanel rendered="!LEN(mapfieldsWrap[index].fieldError) > 0}"><strong>{!$Label.ia_crm__Error}: </strong></apex:outputpanel>
                                                                                        {!mapfieldsWrap[index].fieldError}
                                                                                    </div>
                                                                                </div>
                                                                            </apex:outputpanel>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-col--padded slds-size--1-of-1 slds-medium-size--1-of-12 slds-large-size--1-of-12" style="min-width: 178px;">
                                                                <div class="slds-form-element__row  ">
                                                                    <div class="slds-form-element ">
                                                                        <label class="slds-form-element__label" for="select-12">{!$Label.ia_crm__Operator}</label>
                                                                        <div class="slds-form-element__control">
                                                                            <div class="slds-select_container">
                                                                                <apex:selectlist value="{!mapfieldsWrap[index].operationFilter}" disabled="{!IF(mapfieldsWrap[index].isDisabled || mapfieldsWrap[index].isDisabledOperator, 'true', 'false')}" size="1" styleclass="slds-select">
                                                                                    <apex:actionsupport event="onchange" action="{!clearErrorMessage}" rerender="pageMessages" status="fetchStatus" />
                                                                                    <apex:selectoptions value="{!mapfieldsWrap[index].optionsOperator}" />
                                                                                </apex:selectlist>
                                                                            </div>
                                                                            <apex:outputpanel >
                                                                                <div class="slds-has-error">
                                                                                    <div id="error-message" class="slds-form-element__help">
                                                                                        <apex:outputpanel rendered="!LEN(mapfieldsWrap[index].operationError) > 0}"><strong>{!$Label.ia_crm__Error}: </strong></apex:outputpanel>
                                                                                        {!mapfieldsWrap[index].operationError}

                                                                                    </div>
                                                                                </div>
                                                                            </apex:outputpanel>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-col--padded slds-size--1-of-2 slds-medium-size--3-of-12 slds-large-size--3-of-12">
                                                                <div class="slds-form-element__row ">
                                                                    <div class="slds-form-element">
                                                                        <label class="slds-form-element__label" for="input-02">{!$Label.ia_crm__Value}</label>
                                                                        <apex:outputpanel rendered="{!mapfieldsWrap[index].toFieldType == 'BOOLEAN' || mapfieldsWrap[index].toFieldType == 'PICKLIST'}">
                                                                            <div class="slds-form-element__control">
                                                                                <div class="slds-select_container">
                                                                                    <apex:selectlist value="{!mapfieldsWrap[index].valueFilter}" disabled="{!IF(mapfieldsWrap[index].isDisabled, 'true', 'false')}" size="1" styleclass="slds-select">
                                                                                        <apex:actionsupport event="onchange" action="{!clearErrorMessage}" rerender="pageMessages" status="fetchStatus" />
                                                                                        <apex:selectoptions value="{!mapfieldsWrap[index].picklistOptions}" />
                                                                                    </apex:selectlist>
                                                                                </div>
                                                                            </div>
                                                                        </apex:outputpanel>
                                                              			 <apex:outputpanel rendered="{!mapfieldsWrap[index].toFieldType == 'MULTIPICKLIST'}" id="multipicklistPanel">	
			                                                     		 	<c:MultiSelectList availableoptions="{!mapfieldsWrap[index].multiPicklistOptions}"
			                                           						selectedoptions="{!mapfieldsWrap[index].selectedOptions}"
			                                           						controlname="opportunityTransaction" />	 
				                                                     	</apex:outputpanel>
                                                                        <apex:outputpanel rendered="{!mapfieldsWrap[index].toFieldType=='DATE' || mapfieldsWrap[index].toFieldType=='DATETIME'}">
                                                                            <apex:inputtext size="10" id="txtDatetime" value="{!mapfieldsWrap[index].valueFilter}" styleclass="slds-input" >
                                                                                <apex:actionsupport event="onmouseup" action="{!clearErrorMessage}" rerender="pageMessages" status="fetchStatus" />
                                                                            </apex:inputtext>
                                                                            <apex:inputfield html-placeholder="{!$Label.ia_crm__Leave_blank_to_search_for_empty_values}" value="{!myAccount.ia_crm__Synced_With_Intacct_Date__c}" style="display:none" />
                                                                            <script>
                                                                                document.getElementById("{!$Component.txtDatetime}").onfocus = function() {DatePicker.pickDate(false, this, false);};
                                                                            </script>
                                                                        </apex:outputpanel>
                                                                        <apex:outputpanel rendered="{!mapfieldsWrap[index].toFieldType == ''}">
                                                                            <apex:inputtext html-placeholder="{!$Label.ia_crm__Leave_blank_to_search_for_empty_values}" value="{!mapfieldsWrap[index].valueFilter}" styleclass="slds-input" />
                                                                        </apex:outputpanel>
                                                                        <apex:outputpanel rendered="{!mapfieldsWrap[index].toFieldType == 'TEXT' || mapfieldsWrap[index].toFieldType == 'NUMBER'}">
                                                                            <apex:inputtext html-placeholder="{!$Label.ia_crm__Leave_blank_to_search_for_empty_values}" value="{!mapfieldsWrap[index].valueFilter}" disabled="{!IF(mapfieldsWrap[index].isDisabled, 'true', 'false')}" styleclass="slds-input">
                                                                                <apex:actionsupport event="onchange" action="{!clearErrorMessage}" rerender="pageMessages" status="fetchStatus" />
                                                                            </apex:inputText>
                                                                        </apex:outputpanel>
                                                                        <apex:outputpanel rendered="{!mapfieldsWrap[index].toFieldType == 'REFERENCE'}">
                                                                            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon--right">
                                                                                <apex:inputfield id="lookup" required="false" html-placeholder="{!$Label.ia_crm__Leave_blank_to_search_for_empty_values}" value="{!mapfieldsWrap[index].objectType[mapfieldsWrap[index].fromFieldAPIName]}" styleclass="slds-input" style="visibility:hidden;" >
                                                                                    <apex:actionsupport event="onchange" action="{!clearErrorMessage}" rerender="pageMessages" status="fetchStatus" />
                                                                                </apex:inputfield>
                                                                            </div>
                                                                            <script>
                                                                        var lkInput = document.getElementById("{!$Component.lookup}");
                                                                        lkInput.style.visibility = "";
                                                                        var lkSpan = lkInput.parentElement;
                                                                        var lkLink = lkSpan.querySelector("a");
                                                                        lkLink.style.visibility = "";
                                                                        lkLink.style.cursor = "default";
                                                                        lkLink.className = "";
                                                                        lkLink.innerHTML = "<svg style=\"cursor: pointer;pointer-events:auto;\" xmlns=\"http://www.w3.org/2000/svg\" class=\"slds-input__icon\" aria-hidden=\"true\" viewBox=\"0 0 24 24\"><path d=\"M 22.9 20.9 l -6.2 -6.1 c 1.3 -1.8 1.9 -4 1.6 -6.4 c -0.6 -3.9 -3.8 -7.1 -7.8 -7.4 C 5 0.4 0.4 5 1 10.5 c 0.3 4 3.5 7.3 7.4 7.8 c 2.4 0.3 4.6 -0.3 6.4 -1.5 l 6.1 6.1 c 0.3 0.3 0.7 0.3 1 0 l 0.9 -1 c 0.3 -0.3 0.3 -0.7 0.1 -1 Z M 3.7 9.6 c 0 -3.2 2.7 -5.9 5.9 -5.9 s 6 2.7 6 5.9 s -2.7 6 -6 6 s -5.9 -2.6 -5.9 -6 Z\" /></svg>";
                                                                            </script>
                                                                        </apex:outputpanel>
                                                                        <apex:outputpanel rendered="{!LEN(mapfieldsWrap[index].valueError) > 0}">
                                                                            <div class="slds-has-error">
                                                                                <div id="error-message" class="slds-form-element__help"><strong>{!$Label.ia_crm__Error}: </strong>{!mapfieldsWrap[index].valueError}</div>
                                                                            </div>
                                                                        </apex:outputpanel>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <apex:outputpanel rendered="{!mapfieldsWrapSize > index}" styleclass="slds-col--padded slds-size--1-of-1 slds-medium-size--1-of-12 slds-large-size--1-of-12">
                                                                <div>
                                                                    <label class="slds-form-element__label" for="select-12">{!$Label.ia_crm__Logic}</label>
                                                                    <div class="slds-form-element__control">
                                                                        <div class="slds-select_container">
                                                                            <apex:selectlist value="{!mapfieldsWrap[index].andorFilter}" size="1" styleclass="slds-select" disabled="{!OR(mapfieldsWrap[index].isFilterLogicDisabled, mapfieldsWrap[index].isDisabled)}">
                                                                                <apex:actionsupport event="onchange" action="{!clearErrorMessage}" rerender="pageContractTransactionMessages" status="fetchStatus" />
                                                                                <apex:selectoptions value="{!andOrOptions}" />
                                                                            </apex:selectlist>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </apex:outputpanel>
                                                            <apex:outputpanel rendered="{!AND(mapfieldsWrapSize == index, mapfieldsWrapSize > 1)}" styleclass="slds-col--padded slds-size--1-of-1 slds-medium-size--1-of-12 slds-large-size--1-of-12" />
                                                            <apex:outputpanel rendered="{!NOT(mapfieldsWrap[index].isDeleteDisabled)}">
                                                                <div class="slds-col--padded slds-size--1-of-1 slds-medium-size--1-of-12 slds-large-size--1-of-12">
                                                                    <label class="slds-form-element__label" for="select-12"></label>
                                                                    <div class="slds-form-element__control" style="margin-top: 4px;margin-left: -15px;">
                                                                        <apex:commandlink styleclass="slds-button slds-button--icon" action="{!removefilter}" status="fetchStatus" rerender="idFieldFilters, betweenFilter" style="box-shadow: none;" title="{!$Label.ia_crm__Delete}">
