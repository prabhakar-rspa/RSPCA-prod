<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Extension_Date_validation</fullName>
    <active>true</active>
    <errorConditionFormula>Requested_Membership_Extension_Date__c  &lt;=  Account__r.Membership_Expiry_Date__c</errorConditionFormula>
    <errorDisplayField>Requested_Membership_Extension_Date__c</errorDisplayField>
    <errorMessage>Requested Membership Extension Date cannot be prior to the current Membership Expiry Date.</errorMessage>
</ValidationRule>
