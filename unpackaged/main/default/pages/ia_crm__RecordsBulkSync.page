        </div>
    </div>
    <script type="text/javascript">
        function showSpinner() {
           document.getElementById("spinnerContainer").style.display = "block";
        }
        
        document.getElementById("firstTab").onclick = function() {switchTab('1')};
        document.getElementById("secondTab").onclick = function() {switchTab('2')};

        var thirdTab = document.getElementById("thirdTab");
        if(thirdTab != null) {
            thirdTab.onclick = function() {switchTab('3')};
        }

        var forthTab = document.getElementById("forthTab");
        if(forthTab != null) {
            forthTab.onclick = function() {switchTab('4')};
        }
        var fifthTab = document.getElementById("fifthTab");

        if(fifthTab != null) {
            fifthTab.onclick = function() {switchTab('5')};
        }

        var sixTab = document.getElementById("sixTab");

        if(sixTab != null) {
            sixTab.onclick = function() {switchTab('6')};
        }

        function expandOrColapseCreationProcess() {
             var mainDiv = document.getElementById('sectionJournalCreationProcess');

             if(mainDiv.classList.contains('slds-is-collapsed')) {
                 mainDiv.classList.remove('slds-is-collapsed');
             } else {
                 mainDiv.classList.remove('slds-section__content');
                 mainDiv.classList.remove('slds-is-open');
                 mainDiv.className += 'slds-is-collapsed';
             }

             var mainSec = document.getElementById('jeMainSection');
             if(mainSec.classList.contains('slds-is-open')) {
                 mainSec.classList.remove('slds-is-open');
             } else {
                 mainSec.className += ' slds-is-open';
             }
         }

         function expandOrColapseJournalEntry() {
             var mainDiv = document.getElementById('filterSectionJournalEntryDiv');

             if(mainDiv.classList.contains('slds-is-collapsed')) {
                 mainDiv.classList.remove('slds-is-collapsed');
                 mainDiv.className += 'slds-is-open slds-section__content';
             } else {
                 mainDiv.classList.remove('slds-section__content');
                 mainDiv.classList.remove('slds-is-open');
                 mainDiv.className += 'slds-is-collapsed';
             }

             var mainSec = document.getElementById('jeFilterMainSection');
             if(mainSec.classList.contains('slds-is-open')) {
                 mainSec.classList.remove('slds-is-open');
             } else {
                 mainSec.className += ' slds-is-open';
             }
         }

        function expandOrColapseJournalEntryLine() {
            var mainDiv = document.getElementById('filterSectionJournalEntryLineDiv');

            if(mainDiv.classList.contains('slds-is-collapsed')) {
                mainDiv.classList.remove('slds-is-collapsed');
                mainDiv.className += 'slds-is-open slds-section__content';
            } else {
                mainDiv.classList.remove('slds-section__content');
                mainDiv.classList.remove('slds-is-open');
                mainDiv.className += 'slds-is-collapsed';
            }

            var mainSec = document.getElementById('jeFilterLineMainSection');
             if(mainSec.classList.contains('slds-is-open')) {
                 mainSec.classList.remove('slds-is-open');
             } else {
                 mainSec.className += ' slds-is-open';
             }
        }

        function checkAll(cb,receivedInputID) {
            var inputElem = document.getElementsByTagName("input");
            for(var i=0; i<inputElem.length; i++)
            {
                if(inputElem[i].id.indexOf(receivedInputID)!=-1){
                    inputElem[i].checked = cb.checked;
                }
            }
        }
    </script>
    <style>
        .slds-section__content {
            max-height: none !important;
        }
    </style>
</apex:page>