                                                        <path d="M23.7 19.6L13.2 2.5c-.6-.9-1.8-.9-2.4 0L.3 19.6c-.7 1.1 0 2.6 1.1 2.6h21.2c1.1 0 1.8-1.5 1.1-2.6zM12 18.5c-.8 0-1.4-.6-1.4-1.4s.6-1.4 1.4-1.4 1.4.6 1.4 1.4-.6 1.4-1.4 1.4zm1.4-4.2c0 .3-.2.5-.5.5h-1.8c-.3 0-.5-.2-.5-.5v-6c0-.3.2-.5.5-.5h1.8c.3 0 .5.2.5.5v6z" />
                                                    </svg>

                                                </span>
                                            </div>
                                            <div style="position: relative; left: 45px; width: 90%;">
                                                <apex:outputtext escape="false" value="{!$Label.ia_crm__To_ensure_accuracy_with_Intacct}" />
                                            </div>
                                        </div>
                                    </apex:outputpanel>
                                </div>
                                <div class="slds-modal__footer slds-theme--default">
                                    <apex:commandbutton value="{!modalConfirmbutton}" styleclass="slds-button slds-button--brand" status="fetchStatus" action="{!initializeSyncProcess}" rendered="{!modalConfirmbutton != '' && selectedObjectValue != 'syncAccountTransaction' && selectedObjectValue != 'syncContractTransaction' && selectedObjectValue != 'journalEntryCreation'}" rerender="syncButton, modalMessage" />
                                    <apex:commandbutton value="{!modalConfirmbutton}" styleclass="slds-button slds-button--brand" status="fetchStatus" action="{!initializeSyncAccountTransaction}" rendered="{!modalConfirmbutton != '' && selectedObjectValue == 'syncAccountTransaction'}" rerender="syncButton, modalMessage" />
                                    <apex:commandbutton value="{!modalConfirmbutton}" styleclass="slds-button slds-button--brand" status="fetchStatus" action="{!initializeSyncContractTransaction}" rendered="{!modalConfirmbutton != '' && selectedObjectValue == 'syncContractTransaction'}" rerender="syncButton, modalMessage" />
                                    <apex:commandbutton value="{!modalConfirmbutton}" styleclass="slds-button slds-button--brand" status="fetchStatus" action="{!createJournalEntryProcess}" rendered="{!modalConfirmbutton != '' && selectedObjectValue == 'journalEntryCreation'}" rerender="createButton, modalMessage" />
                                    <apex:commandbutton value="{!modalclosebutton}" styleclass="slds-button slds-button--neutral" action="{!closeModal}" rerender="modalMessage, journalEntryFilter, jEFilters, jEntryCreationProcessPanel, recordsPanel, getOpportunityRecordsPanel, paginationRecords" />

                                </div>
                            </div>
                        </div>
                        <div class="slds-backdrop slds-backdrop--open"></div>
                    </apex:outputpanel>
                </apex:outputpanel>

